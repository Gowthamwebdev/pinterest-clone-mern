generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  user_id     Int      @id @default(autoincrement())
  name        String
  email       String   @unique
  password    String
  profile_img String
  created_at  DateTime @default(now())
  deleted_at  DateTime?
}
